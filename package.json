    "deno": "astro build && deno run --allow-net --allow-read --allow-env --watch ./dist/server/entry.mjs",
    "staging": "npm run build && deployctl deploy --project=labnotes --no-static--include=./dist ./dist/server/entry.mjs",
    "prod": "npm run build && deployctl deploy --prod --project=labnotes --no-static--include=./dist ./dist/server/entry.mjs",
    "commit": "git add . && git commit && git push",
